{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/(pages)/(platform)/legit-checker/page.jsx", "app/(pages)/(platform)/lk/addresses/page.jsx", "app/(pages)/(platform)/lk/dashboard/page.jsx", "app/(pages)/(guard-admin)/admin-panel/catalog/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
